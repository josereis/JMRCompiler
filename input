Class exem;
Int: a, b;
Str: c;

final Int max = 10;

Void teste(Int x, Float y, Str nome) {
	Int: d;
	
	for(d=1; d < x; d++) {
		print("O ", nome," eh um viadinho ", d*y, " vezes\n");	
	}	
}

Int fat() {
	return 10;
}

Main {
	print("DIGITE O NUMERO INTEIRO X: ");
	read(a);
	
	b = fat();
	print("\n\nretorno = ", b, "\n\n");
}